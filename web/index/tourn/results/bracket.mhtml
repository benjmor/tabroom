<%args>
	$result_id => undef
	$tourn_id  => undef
</%args>
<%init>

	my $key = $result_id;
	return if $m->cache_self(
		key        => $key,
		expires_in => '5m'
	);

	my $result_set;

	if ($result_id eq int($result_id)) {
		$result_set = Tab::ResultSet->retrieve($result_id);
	}

</%init>

	<& "/funclib/bracket_show.mas",
		result_set => $result_set,
		public     => 1
	&>

<title>
Results - <% $event->name %> - Bracket - <% $tourn->name %> <% $tourn->start->year if $tourn->start %> - Tabroom.com
</title>
